void Swap(int sour1, int dest1, int sour2, int dest2)
{
	int temp = bingo[sour1][dest1];
	bingo[sour1][dest1] = bingo[sour2][dest2];
	bingo[sour2][dest2] = temp;
}


void Shuffle(int n)
{
	for (int i = 0; i < n; i++)
	{
		int sour1 = rand() % 5;
		int dest1 = rand() % 5;
		int sour2 = rand() % 5;
		int dest2 = rand() % 5;

		Swap(sour1, dest1, sour2, dest2);
	}
}
